init = [1.5e-4;1.0;0.0;0.0];
t = linspace(0.0,40.0,40);
function xdot = f(x,t) 

xdot(1) = x(4) .+ -1.0.*x(2).*x(1);
xdot(2) = -1.0.*x(2).*x(1);
xdot(3) = x(4);
xdot(4) = -1.0.*x(4) .+ x(2).*x(1);
endfunction;

function jac = jj(x,t)

jac(1,1) = -1.0.*x(2);
jac(1,2) = -1.0.*x(1);
jac(1,3) = 0.0;
jac(1,4) = 1.0;
jac(2,1) = -1.0.*x(2);
jac(2,2) = -1.0.*x(1);
jac(2,3) = 0.0;
jac(2,4) = 0.0;
jac(3,1) = 0.0;
jac(3,2) = 0.0;
jac(3,3) = 0.0;
jac(3,4) = 1.0;
jac(4,1) = x(2);
jac(4,2) = x(1);
jac(4,3) = 0.0;
jac(4,4) = -1.0;
endfunction;

x = lsode({@f,@jj}, init, t);
save ("-ascii", "-", "x");